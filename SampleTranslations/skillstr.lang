	<string name="skills.attacks.twohanded.crossbow2.desc">Çarpışmada patlayarak %150 Fiziksel hasar veren bir ok fırlat.</string>

	<string name="skills.attacks.twohanded.maul1.name">Topuz Savurma</string>
	<string name="skills.attacks.twohanded.maul1.desc">%175 Fiziksel hasar veren ve %20 şansla \cb0dfffSersemletme\d uygulayan yavaş, ağır bir savurma.</string>
	<string name="skills.attacks.twohanded.maul2.name">Yere Vurma</string>
	<string name="skills.attacks.twohanded.maul2.desc">Bir alanda %250 Fiziksel hasar veren ve \cb0dfffSersemletme\d uygulayan güçlü bir yere vurma.</string>

	<string name="skills.attacks.twohanded.greataxe1.name">Büyük Balta Darbesi</string>
	<string name="skills.attacks.twohanded.greataxe1.desc">%125 Fiziksel hasar veren ve %20 şansla \cb0dfffKanama\d yığını uygulayan geniş bir yırtıcı darbe.</string>
	<string name="skills.attacks.twohanded.greataxe2.name">Kasırga Darbesi</string>
	<string name="skills.attacks.twohanded.greataxe2.desc">Baltanla sürekli dönmek için basılı tut. Alandaki tüm düşmanlara %75 Fiziksel hasar verir.</string>

	<string name="skills.attacks.twohanded.daggers1.name">Çift Dilim</string>
	<string name="skills.attacks.twohanded.daggers1.desc">Her biri %100 Fiziksel hasar veren iki hızlı darbe indir.</string>
	<string name="skills.attacks.twohanded.daggers2.name">Dash Vuruşu</string>
	<string name="skills.attacks.twohanded.daggers2.desc">Yolundaki tüm düşmanlara %200 Fiziksel hasar veren hızlı bir ileri dash yapma.</string>

	<string name="skills.attacks.twohanded.staff.fire1.name">Alev Püskürtme</string>
	<string name="skills.attacks.twohanded.staff.fire1.desc">%25 Fiziksel hasar veren sürekli alev püskürtmek için basılı tut.</string>
	<string name="skills.attacks.twohanded.staff.fire2.name">Ateş Patlaması</string>
	<string name="skills.attacks.twohanded.staff.fire2.desc">Asayı yere vurarak bir alanda %100 Fiziksel hasar veren ateşli bir patlama yarat.</string>

	<string name="skills.attacks.twohanded.staff.ice1.name">Buz Mızrağı</string>
	<string name="skills.attacks.twohanded.staff.ice1.desc">Buz fırlatmak için basılı tut. Buzun menzili, hızı ve hasarı tusa basma süresine göre artar, en fazla %75 Fiziksel hasar verir.</string>
	<string name="skills.attacks.twohanded.staff.ice2.name">Buz Dikenleri</string>
	<string name="skills.attacks.twohanded.staff.ice2.desc">Asayı yere vurarak ileri yönde her biri %150 Fiziksel hasar veren üç buz dikeni fışkırt.</string>

	<string name="skills.attacks.twohanded.staff.lightning1.name">Yıldırım Işını</string>
	<string name="skills.attacks.twohanded.staff.lightning1.desc">%40 Fiziksel hasar veren bir elektrik ışını için basılı tut. Işın, deldiği her düşman için %10 hasar kaybeder.</string>
	<string name="skills.attacks.twohanded.staff.lightning2.name">Gök Gürültüleri</string>
	<string name="skills.attacks.twohanded.staff.lightning2.desc">Asayı yere vurarak önündeki alana her biri %100 Fiziksel hasar veren iki yıldırım çaktır.</string>



	<!-- Paladin -->


	<string name="skills.paladin.spell_1.name">Kutsal Darbe</string>
	<string name="skills.paladin.spell_1.desc">Kutsal güçle kuşanmış darbeler indirerek önündeki düşmanlara Fiziksel Hasar verirsin.</string>
	<string name="skills.paladin.spell_1.up1.name">Kutsal Vuruş</string>
	<string name="skills.paladin.spell_1.up1.desc">Darbenin etki alanı genişler.</string>
	<string name="skills.paladin.spell_1.up2.name">Arındıran Alevler</string>
	<string name="skills.paladin.spell_1.up2.desc">Darbe alanında kutsal alevler parlayarak sürekli Ateş hasarı verir.</string>
	<string name="skills.paladin.spell_1.up3.name">Kutsal Gazap</string>
	<string name="skills.paladin.spell_1.up3.desc">Yakındaki düşmanlar arasında zincirlenebilen ek Yıldırım hasarı verir.</string>

	<string name="skills.paladin.spell_2.name">Şifa Dairesi</string>
	<string name="skills.paladin.spell_2.desc">Kutsal ışıkla kendini ve müttefiklerini iyileştirirsin.</string>
	<string name="skills.paladin.spell_2.up1.name">Kutsanmış Dokunuş</string>
	<string name="skills.paladin.spell_2.up1.desc">Ayrıca kısa süreliğine Can ve Mana Yenilenmesini artıran bir kutsama bahşeder.</string>
	<string name="skills.paladin.spell_2.up2.name">Arındıran Dokunuş</string>
	<string name="skills.paladin.spell_2.up2.desc">Tüm lanetleri ve hastalıkları arındırır. Kısa süreliğine tüm kötü etkilere karşı bağışıklık kazanırsın.</string>
	<string name="skills.paladin.spell_2.up3.name">Kutsal Ateş</string>
	<string name="skills.paladin.spell_2.up3.desc">Alandaki tüm İskeletlere kutsal ateşle Fiziksel hasar verir.</string>

	<string name="skills.paladin.passive_1.name">Kutsal Zırhlanma</string>
	<string name="skills.paladin.passive_1.desc">Kutsal güçler zırhını güçlendirir ve %100 Fiziksel Hasarı engelleme şansı verir.</string>
	<string name="skills.paladin.passive_1.up1.name">Kutsal Kalkan</string>
	<string name="skills.paladin.passive_1.up1.desc">Ateş, Buz ve Yıldırım defansını artırır.</string>
	<string name="skills.paladin.passive_1.up2.name">Kutsal Refleks</string>
	<string name="skills.paladin.passive_1.up2.desc">Herhangi bir hasar türünün %100'ünü engelleme şansın artar.</string>
	<string name="skills.paladin.passive_1.up3.name">Kutsal İntikam</string>
	<string name="skills.paladin.passive_1.up3.desc">Hasar aldığında, zırh değerine göre yakındaki bir düşmana Yıldırım hasarı veren kutsal bir ok fırlatma şansı kazanırsın.</string>

	<string name="skills.paladin.passive_2.name">Kutsal Lütuf</string>
	<string name="skills.paladin.passive_2.desc">\cffdfb0Yakın Dövüş Silahı\d vuruşların kutsal güçle mananı yenileme şansı kazanır.</string>
	<string name="skills.paladin.passive_2.up1.name">Kutsal İrade</string>
	<string name="skills.paladin.passive_2.up1.desc">Canın yarıdan azken büyülerin daha az mana harcar.</string>
	<string name="skills.paladin.passive_2.up2.name">İnanç Bariyeri</string>
	<string name="skills.paladin.passive_2.up2.desc">Maksimum mananın yarısından fazlasına sahipken, mana harcayarak gelen hasarı engellersin.</string>
	<string name="skills.paladin.passive_2.up3.name">Kutsal Gayret</string>
	<string name="skills.paladin.passive_2.up3.desc">\cb0dfffİlahi Lütuf\d tetiklendiğinde mevcut manana göre ek Fiziksel hasar verirsin.</string>


	<string name="skills.paladin.mastery_1.name">Kutsal Adalet</string>
	<string name="skills.paladin.mastery_1.desc"></string>

	<string name="skills.paladin.mastery_1.spell_1.name">Kutsal Yargı</string>
	<string name="skills.paladin.mastery_1.spell_1.desc">Kutsal çekicinle seçilmiş düşmanlarına hüküm verir, bir süre boyunca Fiziksel hasar indirirsin.</string>
	<string name="skills.paladin.mastery_1.spell_1.up1.name">Uzatılmış Yargı</string>
	<string name="skills.paladin.mastery_1.spell_1.up1.desc">\cb0dfffİlahi Yargı\d etkisi altındaki düşmanları öldürdüğünde süreyi uzatma şansı kazanırsın. Süre 7 saniyeden fazla uzatılamaz.</string>
	<string name="skills.paladin.mastery_1.spell_1.up2.name">Kutsal Gazap Çekici</string>
	<string name="skills.paladin.mastery_1.spell_1.up2.desc">Yargı çekicin ek Yıldırım hasarı verir.</string>

	<string name="skills.paladin.mastery_1.passive_1.name">Adaletin Bekçisi</string>
	<string name="skills.paladin.mastery_1.passive_1.desc">Saldırıları blokladığında veya hasar aldığında, saldırgana \cb0dfffSilahsızlandırma\d ve \cb0dfffSuskunluk\d etkisi uygulama şansı kazanırsın.</string>
	<string name="skills.paladin.mastery_1.passive_1.up1.name">Günahkarların Cezası</string>
	<string name="skills.paladin.mastery_1.passive_1.up1.desc">\cb0dfffSilahsızlandırma\d veya \cb0dfffSuskunluk\d etkisi altındaki düşmanlara daha fazla hasar verirsin.</string>
	<string name="skills.paladin.mastery_1.passive_1.up2.name">Kutsal Prangalar</string>
	<string name="skills.paladin.mastery_1.passive_1.up2.desc">\cffdfb0Silah\d ve \cffdfb0Büyü\d vuruşların hedefi ve yakınındaki başka bir düşmanı kutsal zincirlerle bağlama şansı kazanır. Zincirlenmiş düşmanların hareket hızı düşer ve sürekli Fiziksel hasar alırlar.</string>

	<string name="skills.paladin.mastery_2.name">Kutsal Işık</string>
	<string name="skills.paladin.mastery_2.desc"></string>

	<string name="skills.paladin.mastery_2.spell_1.name">Kutsal Sütunlar</string>
	<string name="skills.paladin.mastery_2.spell_1.desc">Göklerden kutsal ışık sütunları çağırarak düşmanlarına Fiziksel hasar verir ve onları \cb0dfffKör Eder\d.</string>
	<string name="skills.paladin.mastery_2.spell_1.up1.name">Kutsal Düzen</string>
	<string name="skills.paladin.mastery_2.spell_1.up1.desc">Kutsal bir düzende art arda inen ek sütunlar çağırırsın. Her ardışık vuruş gücünün yarısını korur.</string>
	<string name="skills.paladin.mastery_2.spell_1.up2.name">Gazap Sütunları</string>
	<string name="skills.paladin.mastery_2.spell_1.up2.desc">Kutsal sütunlar ek Yıldırım hasarı verir.</string>

	<string name="skills.paladin.mastery_2.passive_1.name">Kutsal Muhafız</string>
	<string name="skills.paladin.mastery_2.passive_1.desc">Canın yarıdan azken hasar aldığında, kısa süreliğine \cb0dfffYenilmezlik\d kutsanması kazanma şansın olur. Bu kutsamanın bir bekleme süresi vardır.</string>
	<string name="skills.paladin.mastery_2.passive_1.up1.name">Kutsal Şifa</string>
	<string name="skills.paladin.mastery_2.passive_1.up1.desc">Kutsama etkisi altındayken Can Yenilenmen büyük ölçüde artar.</string>
	<string name="skills.paladin.mastery_2.passive_1.up2.name">Kutsal Misilleme</string>
	<string name="skills.paladin.mastery_2.passive_1.up2.desc">Kutsama kazandığında otomatik olarak \cb0dfffKutsal Sütunlar\d büyüsünü kullanırsın.</string>

	<string name="skills.paladin.mastery_3.name">Kutsal Alev</string>
	<string name="skills.paladin.mastery_3.desc"></string>

	<string name="skills.paladin.mastery_3.spell_1.name">Arındıran Alev</string>
	<string name="skills.paladin.mastery_3.spell_1.desc">Kutsal ateşle yoğurulmuş güçlü bir patlama hazırlarsın. Serbest bıraktığında çevrende bulunan tüm düşmanlara Ateş hasarı verir.</string>
	<string name="skills.paladin.mastery_3.spell_1.up1.name">Kutsal Körük</string>
	<string name="skills.paladin.mastery_3.spell_1.up1.desc">\cb0dfffArındıran Alev\d büyüsünü kullanırken ek \cb0dfffKutsal Ateş\d yığınları kazanırsın.</string>
	<string name="skills.paladin.mastery_3.spell_1.up2.name">Kutsal Mızrak</string>

	<string name="skills.paladin.mastery_3.passive_1.name">Kutsal Alevler</string>
	<string name="skills.paladin.mastery_3.passive_1.desc">Büyü kullandığında \cb0dfffKutsal Ateş\d yığınları oluşturursun. Bu yığınlara sahipken etrafındaki düşmanlara Ateş hasarı veren kutsal bir aura yayarsın.</string>
	<string name="skills.paladin.mastery_3.passive_1.up1.name">Kutsanmış Silahlar</string>
	<string name="skills.paladin.mastery_3.passive_1.up1.desc">Her \cb0dfffKutsal Ateş\d yığını silahlarına Ateş hasarı katar.</string>
	<string name="skills.paladin.mastery_3.passive_1.up2.desc">Her \cb0dfffKutsal Ateş\d yığını Ateş Direncini ve Ateş Nüfuzunu artırır.</string>



	<!-- Ranger -->

	<string name="skills.ranger.spell_1.name">Avcının İşareti</string>
	<string name="skills.ranger.spell_1.desc">Hedef bölgedeki düşmanları işaretleyerek Fiziksel hasar veren bir ok fırlatırsın. \cb0dfffİşaretli\d düşmanlara daha fazla hasar verirsin.</string>
	<string name="skills.ranger.spell_1.up1.name">İz Sürücü</string>
	<string name="skills.ranger.spell_1.up1.desc">\cb0dfffİşaretli\d düşmanlara vurduğunda kısa süreliğine Hareket Hızı artışı kazanırsın.</string>
	<string name="skills.ranger.spell_1.up2.name">Avcı İçgüdüsü</string>
	<string name="skills.ranger.spell_1.up2.desc">\cb0dfffİşaretli\d düşmanları avladığında Can ve Mana yenileme şansı kazanırsın.</string>
	<string name="skills.ranger.spell_1.up3.name">Usta Avcı</string>
	<string name="skills.ranger.spell_1.up3.desc">\cb0dfffİşaretli\d \cb0dfffBoss\d ve \cb0dfffElit\d düşmanlara daha fazla hasar verirsin.</string>

	<string name="skills.ranger.spell_2.desc">Kısa süre sonra tetiklenen bir tuzak yerleştirirsin. Tuzaklar patladığında dar bir alanda Fiziksel hasar verir ve düşmanları \cb0dfffYavaşlatır\d.</string>
	<string name="skills.ranger.spell_2.up1.name">Tuzak Ustası</string>
	<string name="skills.ranger.spell_2.up1.desc">Her kullanımda birden fazla tuzak yerleştirirsin.</string>
	<string name="skills.ranger.spell_2.up2.name">Dikenli Tuzak</string>
	<string name="skills.ranger.spell_2.up2.desc">Tuzaklar artık \cb0dfffKanama\d etkisi uygular.</string>
	<string name="skills.ranger.spell_2.up3.name">Dondurucu Tuzak</string>
	<string name="skills.ranger.spell_2.up3.desc">Tuzaklar ek Buz hasarı verir.</string>

	<string name="skills.ranger.passive_1.name">Avcı Ritmi</string>
	<string name="skills.ranger.passive_1.desc">Silah Kritik Şansı kazanır ve Kritik Vuruşların sana \cb0dfffAvcı Ritmi\d yığınları sağlar. Her \cb0dfffAvcı Ritmi\d yığını Silah Kritik Şansını ve Kritik Hasarını artırır.</string>
	<string name="skills.ranger.passive_1.up1.name">Akıcı Hareket</string>
	<string name="skills.ranger.passive_1.up1.desc">Her \cb0dfffAvcı Ritmi\d yığını Silah Hızını artırır.</string>
	<string name="skills.ranger.passive_1.up2.name">Doğal Akış</string>
	<string name="skills.ranger.passive_1.up2.desc">\cb0dfffAvcı Ritmi\d'nin her yığını Mana maliyetlerini azaltır.</string>
	<string name="skills.ranger.passive_1.up3.name">Avcı Çevikliği</string>
	<string name="skills.ranger.passive_1.up3.desc">Dash yaptığında bir \cb0dfffAvcı Ritmi\d kazanırsın.</string>

	<string name="skills.ranger.passive_2.name">Doğada Yaşam</string>
	<string name="skills.ranger.passive_2.desc">Her hasar aldığında kısa süreliğine alınan hasarı azaltan bir içgüdü kazanırsın. Bu etkinin kısa bir bekleme süresi vardır.</string>
	<string name="skills.ranger.passive_2.up1.name">Çevik Manevra</string>
	<string name="skills.ranger.passive_2.up1.desc">İçgüdü etkisi süresince Sıyrılma yeteneği kazanırsın.</string>
	<string name="skills.ranger.passive_2.up2.name">Avcı Manevraları</string>
	<string name="skills.ranger.passive_2.up2.desc">Hasar aldığında bir dash hakkı kazanırsın. Bu etkinin kısa bir bekleme süresi vardır.</string>
	<string name="skills.ranger.passive_2.up3.name">Doğal İyileşme</string>
	<string name="skills.ranger.passive_2.up3.desc">İçgüdü etkisi süresince Can Yenilenmen artar.</string>


	<string name="skills.ranger.mastery_1.name">Nişancı</string>
	<string name="skills.ranger.mastery_1.desc"></string>

	<string name="skills.ranger.mastery_1.spell_1.name">Ok Yağmuru</string>
	<string name="skills.ranger.mastery_1.spell_1.desc">İleri yönde, yere çarparak Fiziksel hasar veren bir ok yağmuru başlatırsın.</string>
	<string name="skills.ranger.mastery_1.spell_1.up1.name">Ok Fırtınası</string>
	<string name="skills.ranger.mastery_1.spell_1.up1.desc">Ok Yağmuru'nun etki alanını artırır.</string>
	<string name="skills.ranger.mastery_1.spell_1.up2.name">Yıldırım Okları</string>
	<string name="skills.ranger.mastery_1.spell_1.up2.desc">Ok Yağmuru'nu, Yıldırım hasarı veren yıldırım çakmaları takip eder.</string>

	<string name="skills.ranger.mastery_1.passive_1.name">Usta Okçuluk</string>
	<string name="skills.ranger.mastery_1.passive_1.desc">\cffdfb0Yay\d yeteneklerini kullanırken hareket hızı cezası azalır ve \cffdfb0Yay\d yetenekleriyle daha fazla hasar verirsin.</string>
	<string name="skills.ranger.mastery_1.passive_1.up1.name">Hızlı Nişan</string>
	<string name="skills.ranger.mastery_1.passive_1.up1.desc">\cffdfb0Yay\d yetenekleriyle artırılmış yetenek hızı kazanırsın.</string>
	<string name="skills.ranger.mastery_1.passive_1.up2.name">Buz Okları</string>
	<string name="skills.ranger.mastery_1.passive_1.up2.desc">\cffdfb0Yay\d yeteneklerin vuruşta bir buz patlaması tetikleme şansı kazanır ve Buz hasarı verir. Bu etkinin kısa bir bekleme süresi vardır.</string>

	<string name="skills.ranger.mastery_2.name">Hayvan Ustası</string>
	<string name="skills.ranger.mastery_2.desc"></string>

	<string name="skills.ranger.mastery_2.spell_1.name">Ruh Kurtları</string>
	<string name="skills.ranger.mastery_2.spell_1.desc">Yakındaki düşmanları avlayan ve çarptığında Fiziksel ve Buz hasarı veren ruh kurtları çağırırsın.</string>
	<string name="skills.ranger.mastery_2.spell_1.up1.name">Azılı Avcılar</string>
	<string name="skills.ranger.mastery_2.spell_1.up1.desc">Ruh kurtları vuruşta yeni bir düşmana saldırma şansı kazanır.</string>
	<string name="skills.ranger.mastery_2.spell_1.up2.name">Vahşi Ruhlar</string>
	<string name="skills.ranger.mastery_2.spell_1.up2.desc">Düşmanları öldürdüğünde bir ruh kurdu çağırma şansı kazanırsın. Bu etkinin kısa bir bekleme süresi vardır.</string>

	<string name="skills.ranger.mastery_2.passive_1.name">Kurt Yoldaşı</string>
	<string name="skills.ranger.mastery_2.passive_1.desc">Yakındaki düşmanlara Fiziksel hasar veren sadık bir kurt çağırırsın.</string>
	<string name="skills.ranger.mastery_2.passive_1.up1.name">Kurt Sürüsü</string>
	<string name="skills.ranger.mastery_2.passive_1.up1.desc">Ek kurtlar çağırırsın.</string>
	<string name="skills.ranger.mastery_2.passive_1.up2.name">Kış Kurdu</string>
	<string name="skills.ranger.mastery_2.passive_1.up2.desc">Kurtların ek Buz hasarı verir.</string>

	<string name="skills.ranger.mastery_3.name">Orman Muhafızı</string>
	<string name="skills.ranger.mastery_3.desc"></string>

	<string name="skills.ranger.mastery_3.spell_1.name">Sarmaşık Tuzağı</string>
	<string name="skills.ranger.mastery_3.spell_1.desc">Etrafında geniş bir alanda filizlenen ve bir süre kalan kökler çağırırsın. Alandaki düşmanlar Fiziksel hasar alır ve \cb0dfffYavaşlar\d.</string>
	<string name="skills.ranger.mastery_3.spell_1.up1.name">Doğanın Şifası</string>
	<string name="skills.ranger.mastery_3.spell_1.up1.desc">Kökler, alana giren müttefiklere yapışan ve sürekli Can yenileyen doğa özü salar.</string>
	<string name="skills.ranger.mastery_3.spell_1.up2.name">Zehirli Sarmaşıklar</string>
	<string name="skills.ranger.mastery_3.spell_1.up2.desc">Ek Zehir hasarı verir.</string>

	<string name="skills.ranger.mastery_3.passive_1.name">Rüzgar Ustası</string>
	<string name="skills.ranger.mastery_3.passive_1.desc">\cffdfb0Silah\d vuruşların ileri yönde Fiziksel hasar veren bir rüzgar dalgası oluşturur. Bu etkinin kısa bir bekleme süresi vardır.</string>
	<string name="skills.ranger.mastery_3.passive_1.up1.name">Kış Rüzgarı</string>
	<string name="skills.ranger.mastery_3.passive_1.up1.desc">Rüzgar dalgaları ek Buz hasarı verir.</string>
	<string name="skills.ranger.mastery_3.passive_1.up2.name">Rüzgar Avcısı</string>
	<string name="skills.ranger.mastery_3.passive_1.up2.desc">Dash yaptığında her yöne rüzgar dalgaları yayarsın.</string>



	<!-- Rogue -->

	<string name="skills.rogue.spell_1.name">Hançer Yelpazesi</string>
	<string name="skills.rogue.spell_1.desc">Koni şeklinde bir alanda birden fazla hançer fırlatarak Fiziksel hasar verirsin.</string>
	<string name="skills.rogue.spell_1.up1.name">Rikoşet</string>
	<string name="skills.rogue.spell_1.up1.desc">Hançerler duvarlardan ve hedeflerden seker. Her sekmede hız ve hasar azalır.</string>
	<string name="skills.rogue.spell_1.up2.name">Ölümcül İsabet</string>
	<string name="skills.rogue.spell_1.up2.desc">\cb0dfffHançer Yelpazesi\d ile daha yüksek kritik vuruş şansı kazanırsın.</string>
	<string name="skills.rogue.spell_1.up3.name">Zehirli Hançerler</string>
	<string name="skills.rogue.spell_1.up3.desc">Ek Zehir hasarı verir.</string>
	<string name="skills.rogue.spell_1.up4.name">Keskin Nişan</string>
	<string name="skills.rogue.spell_1.up4.desc">Daha az ama daha güçlü hançerler fırlatırsın. Hançerler daha uzun mesafe kat eder.</string>

	<string name="skills.rogue.spell_2.name">Ölüm Dansı</string>
	<string name="skills.rogue.spell_2.desc">Çevrende hızlı bir dizi vuruş gerçekleştirerek tüm düşmanlara Fiziksel hasar verirsin.</string>
	<string name="skills.rogue.spell_2.up1.name">Silah Kıran Vuruşlar</string>
	<string name="skills.rogue.spell_2.up1.desc">\cb0dfffSilahsızlandırma\d etkisi uygulama şansı kazanır.</string>
	<string name="skills.rogue.spell_2.up2.name">Alev Topu Saldırısı</string>
	<string name="skills.rogue.spell_2.up2.desc">Vurulan hedeflere ve yakınlarındaki bir düşmana alev topları isabet eder. Etkilenen düşmanlar yavaşlar ve sürekli Ateş hasarı alır.</string>
	<string name="skills.rogue.spell_2.up3.name">Yıldırım Vuruşu</string>
	<string name="skills.rogue.spell_2.up3.desc">Ek Yıldırım Hasarı verir.</string>

	<string name="skills.rogue.passive_1.name">Keskin Bıçaklar</string>
	<string name="skills.rogue.passive_1.desc">\cffdfb0Yakın Dövüş\d vuruşların \cb0dfffKeskin Bıçaklar\d yığınları kazandırır. Her yığın Silah Hızını artırır. Süre bittiğinde tüm yığınları aynı anda kaybedersin.</string>
	<string name="skills.rogue.passive_1.up1.name">Kan Coşkusu</string>
	<string name="skills.rogue.passive_1.up1.desc">Her yığın Mana Yenilenmeni artırır.</string>
	<string name="skills.rogue.passive_1.up2.name">Çevik Savunma</string>
	<string name="skills.rogue.passive_1.up2.desc">Her yığın Sıyrılma şansını artırır.</string>
	<string name="skills.rogue.passive_1.up3.name">Hızlı Teknik</string>
	<string name="skills.rogue.passive_1.up3.desc">Her yığın Yetenek Hızını artırır ve büyü kullanarak da yığın kazanırsın.</string>

	<string name="skills.rogue.passive_2.name">Gölge Adımı</string>
	<string name="skills.rogue.passive_2.desc">Sıyrılma yeteneği ve birkaç saniyede bir yenilenen garantili bir sıyrılma yığını kazanırsın.</string>
	<string name="skills.rogue.passive_2.up1.name">Kedi Adımı</string>
	<string name="skills.rogue.passive_2.up1.desc">Hareket Hızını artırır.</string>
	<string name="skills.rogue.passive_2.up2.name">Gölgeden Saldırı</string>
	<string name="skills.rogue.passive_2.up2.desc">Sıyrıldıktan sonra kısa süreliğine ek Kritik Şansı kazanırsın.</string>
	<string name="skills.rogue.passive_2.up3.name">Akıcı Hareket</string>
	<string name="skills.rogue.passive_2.up3.desc">Dash yapmak, \cb0dfffGölge Adımı\d'nın garantili sıyrılmasını yenileme şansı verir.</string>


	<string name="skills.rogue.mastery_1.name">Suikastçi</string>
	<string name="skills.rogue.mastery_1.desc"></string>

	<string name="skills.rogue.mastery_1.spell_1.name">Ölümcül Darbe</string>
	<string name="skills.rogue.mastery_1.spell_1.desc">Yüksek Fiziksel hasar veren hızlı ve ölümcül bir darbe indirirsin.</string>
	<string name="skills.rogue.mastery_1.spell_1.up1.name">Gölge Darbesi</string>
	<string name="skills.rogue.mastery_1.spell_1.up1.desc">Darbe, hedefin arkasındaki koni şeklindeki alanda ki düşmanlara da hasar verir. Her zincirlenen hedefte hasar azalır.</string>
	<string name="skills.rogue.mastery_1.spell_1.up2.name">Kanlı Darbe</string>
	<string name="skills.rogue.mastery_1.spell_1.up2.desc">\cb0dfffÖlümcül Darbe\d ile Can Çalma özelliği kazanırsın.</string>

	<string name="skills.rogue.mastery_1.passive_1.name">Gölge Suikastçısı</string>
	<string name="skills.rogue.mastery_1.passive_1.desc">Silah Kritik Hasarını artırır ve düşmanların seni fark etme menzilini azaltır.</string>
	<string name="skills.rogue.mastery_1.passive_1.up1.name">Gölge İnfazcısı</string>
	<string name="skills.rogue.mastery_1.passive_1.up1.desc">\cffdfb0Yakın Dövüş\d kritik vuruşların \cb0dfffDüşük Can\d'daki düşmanları anında öldürme şansı kazanır. Bosslar'a karşı etkisizdir.</string>
	<string name="skills.rogue.mastery_1.passive_1.up2.desc">\cffdfb0Yakın Dövüş\d saldırıların yakındaki rastgele bir düşmana Ana El Silah hasarı verir.</string>

	<string name="skills.rogue.mastery_2.name">Zehir Ustası</string>
	<string name="skills.rogue.mastery_2.desc"></string>

	<string name="skills.rogue.mastery_2.spell_1.name">Zehir Bulutu</string>
	<string name="skills.rogue.mastery_2.spell_1.desc">Kalıcı bir zehir bulutu oluşturursun. Bulut düşman görüşünü engeller ve içindeki düşmanlara Zehir hasarı verir.</string>
	<string name="skills.rogue.mastery_2.spell_1.up1.name">Gizleyen Bulut</string>
	<string name="skills.rogue.mastery_2.spell_1.up1.desc">Sen ve müttefiklerin bulutun içindeyken Sıyrılma kazanırsınız.</string>
	<string name="skills.rogue.mastery_2.spell_1.up2.name">Aşındıran Zehir</string>
	<string name="skills.rogue.mastery_2.spell_1.up2.desc">Buluta giren düşmanlara \cb0dfffZırh Kırma\d etkisi uygulanır.</string>

	<string name="skills.rogue.mastery_2.passive_1.name">Zehir Hakimiyeti</string>
	<string name="skills.rogue.mastery_2.passive_1.desc">Zehir hasarın artar ve \cffdfb0Zehir\d yeteneklerin \cb0dfffSavunmasızlık\d uygulama şansı kazanır.</string>
	<string name="skills.rogue.mastery_2.passive_1.up1.name">Zehirli Kılıç</string>
	<string name="skills.rogue.mastery_2.passive_1.up1.desc">Her seviye başına silahlarına Zehir hasarı eklenir.</string>
	<string name="skills.rogue.mastery_2.passive_1.up2.name">Ölümcül Zehir</string>
	<string name="skills.rogue.mastery_2.passive_1.up2.desc">Zehir Penetrasyonunu ve Zehir Defansını artırır.</string>

	<string name="skills.rogue.mastery_3.name">Sabotaj Ustası</string>
	<string name="skills.rogue.mastery_3.desc"></string>

	<string name="skills.rogue.mastery_3.spell_1.name">Gölge Bombası</string>
	<string name="skills.rogue.mastery_3.spell_1.desc">İleri doğru, kısa sürede patlayarak Fiziksel hasar veren bir bomba fırlatırsın.</string>
	<string name="skills.rogue.mastery_3.spell_1.up1.name">Alev Bombası</string>
	<string name="skills.rogue.mastery_3.spell_1.up1.desc">Bomba temas anında patlar ve kısa süreliğine Ateş hasarı veren bir alev sahası oluşturur.</string>
	<string name="skills.rogue.mastery_3.spell_1.up2.name">Şok Bombası</string>
	<string name="skills.rogue.mastery_3.spell_1.up2.desc">Bomba, yakındaki düşmanlara zincirlenen ek Yıldırım hasarı verir.</string>
	<string name="skills.rogue.mastery_3.spell_1.up3.name">Kaçış Bombası</string>
	<string name="skills.rogue.mastery_3.spell_1.up3.desc">Bir düşmanın yakınında dash kullandığında otomatik olarak bir bomba bırakırsın. Bu etkinin kısa bir bekleme süresi vardır.</string>

	<string name="skills.rogue.mastery_3.passive_1.name">Gizli Teçhizat</string>
	<string name="skills.rogue.mastery_3.passive_1.desc">Zırhın ve Fiziksel hasarın artar. Hasar aldığında yakındaki düşmanlara \cb0dfffKörlük\d uygulayan bir flaş bombası atarsın. Bu etkinin kısa bir bekleme süresi vardır.</string>
	<string name="skills.rogue.mastery_3.passive_1.up1.name">Şok Aygıtı</string>
	<string name="skills.rogue.mastery_3.passive_1.up1.desc">\cffdfb0Silah\d yeteneklerin yakındaki rastgele bir düşmanı sersemletecek bir yıldırım oku fırlatma şansı kazanır ve Yıldırım hasarı verir.</string>
	<string name="skills.rogue.mastery_3.passive_1.up2.name">Şarapnel Yağmuru</string>
	<string name="skills.rogue.mastery_3.passive_1.up2.desc">Büyü kullanımın her yöne şarapnel fırlatma şansı kazanır ve Fiziksel ve Ateş hasarı verir.</string>




	<!-- Sorcerer -->

	<string name="skills.sorcerer.spell_1.name">Enerji Patlaması</string>
	<string name="skills.sorcerer.spell_1.desc">Koni şeklinde bir alanda yıldırım fırlatarak Yıldırım hasarı verirsin.</string>
	<string name="skills.sorcerer.spell_1.up1.name">Zincir Yıldırım</string>
	<string name="skills.sorcerer.spell_1.up1.desc">Yıldırım ek hedeflere sıçrar.</string>
	<string name="skills.sorcerer.spell_1.up2.name">Güçlü Deşarj</string>
	<string name="skills.sorcerer.spell_1.up2.desc">Büyünün menzili artar.</string>
	<string name="skills.sorcerer.spell_1.up3.name">Alev Işınları</string>
	<string name="skills.sorcerer.spell_1.up3.desc">Hasar türü Ateş'e dönüşür ve ışınlar hedeflere penetre eder.</string>
	<string name="skills.sorcerer.spell_1.up4.name">Buz Işınları</string>
	<string name="skills.sorcerer.spell_1.up4.desc">Daha dar bir alanda daha az ışın fırlatır, ancak düşmanlar birden fazla ışından hasar alabilir. Hasar türü Buz'a dönüşür.</string>

	<string name="skills.sorcerer.spell_2.name">Elementler Küresi</string>
	<string name="skills.sorcerer.spell_2.desc">Yavaşça hareket eden ve yakındaki düşmanlara tüm element hasarlarını veren bir küre çağırırsın.</string>
	<string name="skills.sorcerer.spell_2.up1.name">Odaklanmış Küre</string>
	<string name="skills.sorcerer.spell_2.up1.desc">Büyüyü kullanırken \cb0dfffElement Uyumu\d'na sahipsen küre sadece o elementin hasarını verir.</string>
	<string name="skills.sorcerer.spell_2.up2.name">Kalıcı Küre</string>
	<string name="skills.sorcerer.spell_2.up2.desc">Kürenin süresi uzar.</string>
	<string name="skills.sorcerer.spell_2.up3.name">Element Fırtınası</string>
	<string name="skills.sorcerer.spell_2.up3.desc">Küre her yöne daha küçük büyüler fırlatmaya başlar. Bu büyüler daha az hasar verir.</string>

	<string name="skills.sorcerer.passive_1.name">Element Uyumu</string>
	<string name="skills.sorcerer.passive_1.desc">\cffdfb0Büyü\d kullandığında, o büyünün elementi ile \cb0dfffElement Uyumu\d kazanırsın. \cb0dfffElement Uyumu\d süresince o elementin hasarını artırırsın. Aynı anda tek bir element aktif olabilir.</string>
	<string name="skills.sorcerer.passive_1.up1.name">Elementlerin Kalkanı</string>
	<string name="skills.sorcerer.passive_1.up1.desc">Her seviye başına Ateş, Buz ve Yıldırım Direnci kazanırsın.</string>
	<string name="skills.sorcerer.passive_1.up2.name">Elementlerin Nüfuzu</string>
	<string name="skills.sorcerer.passive_1.up2.desc">Ateş, Buz ve Yıldırım Penetrasyonu kazanırsın.</string>
	<string name="skills.sorcerer.passive_1.up3.name">Elementlerin Dengesi</string>
	<string name="skills.sorcerer.passive_1.up3.desc">\cb0dfffElement Uyumu\d süresince diğer elementlerin hasar bonusu ikiye katlanır.</string>

	<string name="skills.sorcerer.passive_2.name">Büyücü Kanı</string>
	<string name="skills.sorcerer.passive_2.desc">Büyü Kritik Şansı kazanırsın.</string>
	<string name="skills.sorcerer.passive_2.up1.name">Dizginsiz Güç</string>
	<string name="skills.sorcerer.passive_2.up1.desc">Büyü Kritik Hasarın artar.</string>
	<string name="skills.sorcerer.passive_2.up2.name">Güç Dalgası</string>
	<string name="skills.sorcerer.passive_2.up2.desc">Büyü Kritik Vuruşu yaptığında Silah, Yetenek ve Hareket Hızı artışı kazanırsın.</string>
	<string name="skills.sorcerer.passive_2.up3.name">Mana Dalgası</string>
	<string name="skills.sorcerer.passive_2.up3.desc">Büyü Kritik Vuruşu yaptığında mananın bir kısmını geri kazanırsın.</string>


	<string name="skills.sorcerer.mastery_1.name">Ateş Büyücüsü</string>
	<string name="skills.sorcerer.mastery_1.desc"></string>

	<string name="skills.sorcerer.mastery_1.spell_1.name">Alev Duvarı</string>
	<string name="skills.sorcerer.mastery_1.spell_1.desc">Önünde yakıcı bir alev duvarı oluşturarak yakındaki düşmanlara Ateş hasarı verirsin.</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up1.name">Alev Yürüyüşü</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up1.desc">Duvardan geçerken sen ve müttefiklerin, çevredeki düşmanlara Ateş hasarı veren geçici bir alev aurası kazanır.</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up2.name">Alev İzi</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up2.desc">Dash yaptığında bir alev duvarı oluşturursun.</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up3.name">Duvar Kontrolü</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up3.name">Duvarın konumunu hedefleyebilirsin.</string>

	<string name="skills.sorcerer.mastery_1.passive_1.name">Ateş Hakimiyeti</string>
	<string name="skills.sorcerer.mastery_1.passive_1.desc">Ateş hasarın artar ve \cffdfb0Ateş\d büyülerin \cb0dfffZayıflık\d uygulama şansı kazanır.</string>
	<string name="skills.sorcerer.mastery_1.passive_1.up1.name">Patlayan Alevler</string>
	<string name="skills.sorcerer.mastery_1.passive_1.up1.desc">\cffdfb0Ateş\d büyülerin ek Ateş hasarı veren bir patlama tetikleme şansı kazanır. Bu etkinin kısa bir bekleme süresi vardır.</string>
	<string name="skills.sorcerer.mastery_1.spell_1.up3.desc">Duvarın konumu artık hedeflenebilir.</string>
	<string name="skills.sorcerer.mastery_1.passive_1.up2.name">Şifa Alevleri</string>
	<string name="skills.sorcerer.mastery_1.passive_1.up2.desc">Büyü Kritik Vuruşu yaptığında canının bir kısmını geri kazanırsın.</string>

	<string name="skills.sorcerer.mastery_2.name">Buz Büyücüsü</string>
	<string name="skills.sorcerer.mastery_2.desc"></string>

	<string name="skills.sorcerer.mastery_2.spell_1.name">Buz Patlaması</string>
	<string name="skills.sorcerer.mastery_2.spell_1.desc">Her yöne buz parçacıkları fırlatarak Buz hasarı verirsin.</string>
	<string name="skills.sorcerer.mastery_2.spell_1.up1.name">Buz Füzeleri</string>
	<string name="skills.sorcerer.mastery_2.spell_1.up1.desc">Buz parçacıkları, hedefine çarptıktan sonra yeni hedefler arar.</string>
	<string name="skills.sorcerer.mastery_2.spell_1.up2.name">Buz Serpintisi</string>
	<string name="skills.sorcerer.mastery_2.spell_1.up2.desc">Buz parçacıkları düşmanları öldürdüğünde daha küçük bir buz patlaması yaratma şansı kazanır.</string>

	<string name="skills.sorcerer.mastery_2.passive_1.name">Buz Hakimiyeti</string>
	<string name="skills.sorcerer.mastery_2.passive_1.desc">Buz hasarın artar ve \cffdfb0Buz\d büyülerin \cb0dfffDondurma\d uygulama şansı kazanır.</string>
	<string name="skills.sorcerer.mastery_2.passive_1.up1.name">Kristal Parçalanması</string>
	<string name="skills.sorcerer.mastery_2.passive_1.up1.desc">\cffdfb0Buz\d büyüleriyle düşmanları öldürdüğünde Buz hasarı veren bir patlama yaratma şansı kazanırsın.</string>
	<string name="skills.sorcerer.mastery_2.passive_1.up2.name">Buz Zırhı</string>
	<string name="skills.sorcerer.mastery_2.passive_1.up2.desc">Fiziksel ve Buz hasarını engelleme şansı kazanırsın.</string>

	<string name="skills.sorcerer.mastery_3.name">Yıldırım Büyücüsü</string>
	<string name="skills.sorcerer.mastery_3.desc"></string>

	<string name="skills.sorcerer.mastery_3.spell_1.name">Yıldırım Küresi</string>
	<string name="skills.sorcerer.mastery_3.spell_1.desc">Yolundaki tüm düşmanlara Yıldırım hasarı veren bir enerji küresi şarj edersin. Kürenin hasarı şarj süresine göre artar.</string>
	<string name="skills.sorcerer.mastery_3.spell_1.up1.name">Güçlenen Küre</string>
	<string name="skills.sorcerer.mastery_3.spell_1.up1.desc">Vurulan her düşman enerji küresinin süresini uzatır.</string>
	<string name="skills.sorcerer.mastery_3.spell_1.up2.name">Elektrik Alanı</string>
	<string name="skills.sorcerer.mastery_3.spell_1.up2.desc">Kalıcı ve ek Yıldırım hasarı veren bir elektrik alanı oluşturursun.</string>

	<string name="skills.sorcerer.mastery_3.passive_1.name">Yıldırım Hakimiyeti</string>
	<string name="skills.sorcerer.mastery_3.passive_1.desc">Yıldırım hasarın artar ve \cffdfb0Yıldırım\d büyülerin \cb0dfffSersemletme\d uygulama şansı kazanır.</string>
	<string name="skills.sorcerer.mastery_3.passive_1.up1.name">Ark Yıldırımı</string>
	<string name="skills.sorcerer.mastery_3.passive_1.up1.desc">\cffdfb0Yıldırım\d büyülerin düşmanlar arasında zincirlenip Yıldırım hasarı veren bir elektrik arkı oluşturma şansı kazanır.</string>
	<string name="skills.sorcerer.mastery_3.passive_1.up2.name">Statik Alan</string>
	<string name="skills.sorcerer.mastery_3.passive_1.up2.desc">Menzilli saldırıları engelleme şansı kazanırsın. Bu etkinin kısa bir bekleme süresi vardır.</string>











	<!-- Warlock -->

	<string name="skills.warlock.spell_1.name">İblis Kırbacı</string>
	<string name="skills.warlock.spell_1.desc">Şeytani bir kırbaç çağırarak ileri doğru savurur, Fiziksel hasar verir ve \cb0dfffYavaşlatma\d uygularsın.</string>
	<string name="skills.warlock.spell_1.up1.name">Yıldırım Kırbacı</string>
	<string name="skills.warlock.spell_1.up1.desc">Yakındaki düşmanlara yıldırım arkları sıçrayarak Yıldırım hasarı verir.</string>
	<string name="skills.warlock.spell_1.up2.name">Cehennem Kırbacı</string>
	<string name="skills.warlock.spell_1.up2.desc">Kırbaç yolunda kalıcı alevler bırakarak Ateş hasarı verir.</string>
	<string name="skills.warlock.spell_1.up3.name">Şeytan Darbesi</string>
	<string name="skills.warlock.spell_1.up3.desc">Kırbacın ucuna çarpan düşmanlar \cb0dfffSersemletme\d etkisi alır.</string>

	<string name="skills.warlock.spell_2.name">Karanlık Ayin</string>
	<string name="skills.warlock.spell_2.desc">Birkaç saniye süren karanlık bir ayin başlatırsın. Alandaki düşmanlar sürekli Fiziksel hasar alır ve ayin tamamlandığında güçlü bir son darbe vurulur. Ayini tamamlamadan alandan ayrılırsan büyü sona erer.</string>
	<string name="skills.warlock.spell_2.up1.name">Kan Ayini</string>
	<string name="skills.warlock.spell_2.up1.desc">Alandaki düşmanlara Kanama yığınları uygulama şansı kazanırsın.</string>
	<string name="skills.warlock.spell_2.up2.name">Cehennem Ateşi</string>
	<string name="skills.warlock.spell_2.up2.desc">Hasar türü Ateş'e dönüşür ve ayin tamamlandığında daha güçlü bir patlama yaratır.</string>
	<string name="skills.warlock.spell_2.up3.name">Ruh Emme Ayini</string>
	<string name="skills.warlock.spell_2.up3.desc">Sen ve müttefiklerin ayin alanında \cb0dfffYaşam Çalma\d kazanırsınız.</string>

	<string name="skills.warlock.passive_1.name">Ruh Emme</string>
	<string name="skills.warlock.passive_1.desc">\cffdfb0Yakın Dövüş\d vuruşların \cb0dfffÖz\d yığınları kazandırır. Her yığın Maksimum Can ve Mananı artırır. Bu etkinin kısa bir bekleme süresi vardır.</string>
	<string name="skills.warlock.passive_1.up1.name">Can Emici</string>
	<string name="skills.warlock.passive_1.up1.desc">\cb0dfffÖz\d yığınları kazandığında Canının bir kısmını geri kazanırsın.</string>
	
	<string name="skills.warlock.passive_1.up3.desc">\cb0dfffÖz\d çektiğin hedeflere \cb0dfffKanama\d yığınları uygularsın.</string>
	<string name="skills.warlock.passive_1.up4.name">Ruh Hasatı</string>
	<string name="skills.warlock.passive_1.up4.desc">Düşmanları öldürdüğünde \cb0dfffÖz\d yığınları kazanma şansın olur.</string>

	<string name="skills.warlock.passive_2.name">Lanet Aurası</string>
	<string name="skills.warlock.passive_2.desc">Aurandaki düşmanlar daha fazla hasar alır.</string>
	<string name="skills.warlock.passive_2.up1.name">İşkence Aurası</string>
	<string name="skills.warlock.passive_2.up1.desc">Aurandaki düşmanlar da daha fazla hasar alır.</string>
	<string name="skills.warlock.passive_2.up2.name">Çürüme Yayılımı</string>
	<string name="skills.warlock.passive_2.up2.desc">\cffdfb0Yakın Dövüş\d vuruşların yakındaki başka bir düşmana ek Zehir hasarı verir.</string>
	<string name="skills.warlock.passive_2.up3.name">Cehennem Ateşi</string>
	<string name="skills.warlock.passive_2.up3.desc">\cffdfb0Büyü\d kullanımın aurandaki tüm düşmanlara Ateş hasarı verir.</string>


	<string name="skills.warlock.mastery_1.name">İblis Terbiyecisi</string>
	<string name="skills.warlock.mastery_1.desc"></string>

	<string name="skills.warlock.mastery_1.spell_1.name">İblis Çağrısı</string>
	<string name="skills.warlock.mastery_1.spell_1.desc">\cb0dfffÖz\d yığınlarını harcayarak iblisleri çağırmak için büyü yaparsın. İblisler yakındaki düşmanlara saldırarak Fiziksel hasar verir.</string>
	<string name="skills.warlock.mastery_1.spell_1.up1.name">Cehennem İblisleri</string>
	<string name="skills.warlock.mastery_1.spell_1.up1.desc">İblisler ek Ateş hasarı verir ve hedeflerine alev topları fırlatır.</string>
	<string name="skills.warlock.mastery_1.spell_1.up2.name">Ölüm Patlaması</string>
	<string name="skills.warlock.mastery_1.spell_1.up2.desc">İblisler öldüklerinde patlayarak çevrelerine Fiziksel hasar verir.</string>

	<string name="skills.warlock.mastery_1.passive_1.name">Gargoyle Kölesi</string>
	<string name="skills.warlock.mastery_1.passive_1.desc">Yanında savaşan bir gargoyle çağırırsın. Gargoyle pençeleri ve üçlü mızrağıyla düşmanlara Fiziksel hasar verir.</string>
	<string name="skills.warlock.mastery_1.passive_1.up1.name">Cehennem Pençeleri</string>
	<string name="skills.warlock.mastery_1.passive_1.up1.desc">Gargoyle'ın pençe saldırıları ek Ateş hasarı verir.</string>
	<string name="skills.warlock.mastery_1.passive_1.up2.name">Fırtına Mızrağı</string>
	<string name="skills.warlock.mastery_1.passive_1.up2.desc">Gargoyle'ın üçlü mızrağı düşmanları sürekli sersemletir ve ek Yıldırım hasarı verir.</string>

	<string name="skills.warlock.mastery_2.name">Ruh Büyücüsü</string>
	<string name="skills.warlock.mastery_2.desc"></string>

	<string name="skills.warlock.mastery_2.spell_1.name">Ruh Kasırgası</string>
	<string name="skills.warlock.mastery_2.spell_1.desc">Etrafında dönen lanetli bir ruh kasırgası oluşturarak alandaki tüm düşmanlara Fiziksel ve Zehir hasarı verirsin.</string>
	<string name="skills.warlock.mastery_2.spell_1.up1.name">Fırtına Kasırgası</string>
	<string name="skills.warlock.mastery_2.spell_1.up1.desc">Kasırga çevresine yıldırım çakmaları çağırarak ek Yıldırım hasarı verir.</string>
	<string name="skills.warlock.mastery_2.spell_1.up2.name">Ruh Örtüsü</string>
	<string name="skills.warlock.mastery_2.spell_1.up2.desc">Büyü süresince Sıyrılma kazanırsın.</string>

	<string name="skills.warlock.mastery_2.passive_1.name">Ruh Efendisi</string>
	<string name="skills.warlock.mastery_2.passive_1.desc">\cb0dfffÖz\d yığınların varken Ana El Silahınla yakındaki rastgele bir düşmana Ana El hasarı veren bir ruh fırlatırsın.</string>
	<string name="skills.warlock.mastery_2.passive_1.up1.name">Ruh Patlaması</string>
	<string name="skills.warlock.mastery_2.passive_1.up1.desc">Ruhlar ek hasar veren bir patlama yaratma şansı kazanır. Bu etkinin kısa bir bekleme süresi vardır.</string>
	<string name="skills.warlock.mastery_2.passive_1.up2.name">Hırslı Ruhlar</string>
	<string name="skills.warlock.mastery_2.passive_1.up2.desc">Ruhlar hedefine çarptıktan sonra ek hedeflere sıçrar.</string>

	<string name="skills.warlock.mastery_3.name">Kaos Büyücüsü</string>
	<string name="skills.warlock.mastery_3.desc"></string>

	<string name="skills.warlock.mastery_3.spell_1.name">Kaos Kılıcı</string>
	<string name="skills.warlock.mastery_3.spell_1.desc">Önündeki alanı biçmek için karanlık bir kılıç çağırarak Fiziksel ve Yıldırım hasarı verirsin.</string>
	<string name="skills.warlock.mastery_3.spell_1.up1.name">Kaos Bıçakları</string>
